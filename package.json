  "private": true,
  "dependencies": {
    "eslint": "^3.19.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-foundation": "^0.7.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "react-scripts": "0.9.5",
    "husky": "^0.13.3",
